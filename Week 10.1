import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner input=new Scanner(System.in);
        int n=input.nextInt();
        ArrayList<Integer> a=new ArrayList<Integer>();
        for(int i=0;i<n;i++){
            int c=input.nextInt();
            a.add(c);
        }
        System.out.println("ArrayList: "+a);
        System.out.print("First : "+a.get(0)+","+" Last : "+a.get(n-1));
    }
}
